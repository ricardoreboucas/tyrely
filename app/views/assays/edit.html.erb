<h1>Editing assay</h1>

<% form_for(@assay) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :fleet_id %><br />
    <%= f.text_field :fleet_id %>
  </p>
  <p>
    <%= f.label :done_on %><br />
    <%= f.datetime_select :done_on %>
  </p>
  <p>
    <%= f.label :doer %><br />
    <%= f.text_field :doer %>
  </p>
  <p>
    <%= f.label :tyres %><br />
    <%= f.text_field :tyres %>
  </p>
  <p>
    <%= f.label :vehicles %><br />
    <%= f.text_field :vehicles %>
  </p>
  <p>
    <%= f.label :created_at %><br />
    <%= f.datetime_select :created_at %>
  </p>
  <p>
    <%= f.label :updated_at %><br />
    <%= f.datetime_select :updated_at %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @assay %> |
<%= link_to 'Back', assays_path %>